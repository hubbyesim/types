{
  "name": "@hubbyesim/types",
  "version": "2.1.13",
  "description": "Type definitions for the hubbyesim project",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./base": {
      "import": "./dist/base/index.js",
      "require": "./dist/base/index.cjs",
      "types": "./dist/base/index.d.ts",
      "default": "./dist/base/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "jest",
    "generate-exports": "node scripts/generate-exports.js"
  },
  "author": "Daniel Fortuyn",
  "license": "ISC",
  "dependencies": {
    "zod": "^3.24.3"
  },
  "peerDependencies": {
    "firebase-admin": "^12.2.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "tsup": "^7.2.0",
    "typescript": "^5.3.2"
  }
}